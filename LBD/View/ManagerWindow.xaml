<Window x:Class="LBD.View.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ViewModels="clr-namespace:LBD.ViewModel"

        xmlns:local="clr-namespace:LBD.View"
        mc:Ignorable="d"
        Title="{Binding ManagerName}" Height="450" Width="800">

    <Window.DataContext>
        <ViewModels:ManagerWindowViewModel/>
    </Window.DataContext>

    <Grid>
        <Button Content="Все кассеты" HorizontalAlignment="Left" Margin="10,81,0,0" VerticalAlignment="Top" Width="136" Height="24" Click="Button_Click"/>
        <Frame NavigationUIVisibility="Hidden" Content="" HorizontalAlignment="Left" Height="410" Margin="151,0,0,0" VerticalAlignment="Top" Width="633" x:Name="Frame1"/>
        <Button Content="Сформировать заказ" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="136" Height="24" Click="Button_Click_1"/>
        <Button Content="Все заказы" HorizontalAlignment="Left" Margin="10,139,0,0" VerticalAlignment="Top" Width="136" Height="24" Click="Button_Click_2"/>

    </Grid>
</Window>
